          "instant": true,
          "interval": "",
          "legendFormat": "",
          "refId": "podAge"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-rancher"
          },
          "expr": "kube_pod_container_resource_requests{namespace=~\"$namespace\",pod=~\"purdue-af-.*\",resource=\"cpu\",container=\"notebook\"}",
          "format": "table",
          "instant": true,
          "interval": "",
          "legendFormat": "",
          "refId": "cpuRequest"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-rancher"
          },
          "expr": "kube_pod_container_resource_requests{namespace=~\"$namespace\",pod=~\"purdue-af-.*\",resource=\"nvidia_com_mig_1g_5gb\",container=\"notebook\"}",
          "format": "table",
          "instant": true,
          "interval": "",
          "legendFormat": "",
          "refId": "gpuRequest"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-rancher"
          },
          "expr": "kube_pod_container_resource_requests{namespace=~\"$namespace\",pod=~\"purdue-af-.*\",resource=\"memory\",container=\"notebook\"}",
          "format": "table",
          "instant": true,
          "interval": "",
          "legendFormat": "",
          "refId": "memRequest"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-rancher"
          },
          "expr": "sum(\n    irate(container_cpu_usage_seconds_total{namespace=~\"$namespace\",pod=~\"purdue-af-.*\", container=\"notebook\"}[5m])\n) by (pod)\n    /\nsum by (pod)(\n    kube_pod_container_resource_requests{namespace=~\"$namespace\",pod=~\"purdue-af-.*\", resource=\"cpu\", container=\"notebook\"}\n)\n",
          "format": "table",
          "instant": true,
          "interval": "",
          "legendFormat": "",
          "refId": "podCpuUtilCurrent"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-rancher"
          },
          "expr": "sum by (pod)(\n    container_memory_working_set_bytes{namespace=~\"$namespace\", pod=~\"purdue-af-.*\", container=\"notebook\"}\n) /\nsum by (pod)(\n    kube_pod_container_resource_requests{namespace=~\"$namespace\", pod=~\"purdue-af-.*\", resource=\"memory\", container=\"notebook\"}\n)\n",
